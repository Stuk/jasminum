{
  "name": "jasminum",
  "version": "0.0.2",
  "description": "A light, modular, promissory, isomorphic Jasmine test scaffold clone",
  "keywords": [
    "jasmine",
    "test",
    "testing",
    "bdd",
    "promise",
    "isomorphic"
  ],
  "main": "jasminum.js",
  "bin": {
    "jasminum": "runner.js",
    "jasminum-phantom": "phantom/runner.js"
  },
  "browser": {
    "platform-reporter": "browser/reporter"
  },
  "dependencies": {
    "q": "^1.0.0",
    "q-io": "^1.11.0",
    "collections": "^1.0.2",
    "glob": "^3.2.8",
    "colors": "^0.6.2",
    "optimist": "^0.6.1",
    "qs": "^0.6.6",
    "mr": "^0.15.4",
    "joey": "^1.5.1",
    "url": "^0.7.9",
    "util": "^0.10.3"
  },
  "scripts": {
    "test": "node runner.js test && node phantom/runner.js test"
  }
}
