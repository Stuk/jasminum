{
  "name": "jasminum",
  "version": "0.0.4",
  "description": "A light, modular, promissory, isomorphic Jasmine test scaffold clone",
  "keywords": [
    "jasmine",
    "test",
    "testing",
    "bdd",
    "promise",
    "isomorphic"
  ],
  "main": "jasminum.js",
  "bin": {
    "jasminum": "runner.js",
    "jasminum-phantom": "phantom/runner.js"
  },
  "browser": {
    "platform-reporter": "browser/reporter"
  },
  "dependencies": {
    "q": ">=1.0.0 <2.0.0",
    "q-io": ">=1.11.0 <2.0.0",
    "collections": ">=1.0.2 <2.0.0",
    "glob": ">=3.2.8 <4.0.0",
    "colors": ">=0.6.2 <0.7.0",
    "optimist": ">=0.6.1 <0.7.0",
    "qs": ">=0.6.6 <0.7.0",
    "mr": ">=0.15.4 <0.16.0",
    "joey": ">=1.5.1 <2.0.0",
    "url": ">=0.7.9 <0.8.0",
    "util": ">=0.10.3 <0.11.0"
  },
  "scripts": {
    "test": "node runner.js test"
  }
}
