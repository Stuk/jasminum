{
  "name": "jasminum",
  "version": "0.0.0",
  "description": "A light, modular, promissory, isomorphic Jasmine test scaffold clone",
  "keywords": [
    "jasmine",
    "test",
    "testing",
    "bdd",
    "promise",
    "isomorphic"
  ],
  "main": "jasminum.js",
  "bin": {
    "jasminum": "runner.js",
    "jasminum-phantom": "phantom/runner.js"
  },
  "browser": {
    "reporter": "browser/reporter"
  },
  "dependencies": {
    "q": "~2.0.0",
    "collections": "~2.0.0",
    "mr": "~2.0.0",
    "glob": "~3.2.8",
    "colors": "~0.6.2",
    "optimist": "~0.6.1",
    "qs": "~0.6.6"
  },
  "scripts": {
    "test": "node runner.js test/jasmine-test.js",
    "test:phantom": "node phantom/runner.js test/jasmine-test.js"
  }
}
